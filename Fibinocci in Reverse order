Time Complexity : O(N)

import java.util.*;

class Solution
{
   void Fibinocci_in_Reverse_order(int n, int a, int b)
     {
      	if (n > 0) {
		Fibinocci_in_Reverse_order(n - 1, b, a + b);
		System.out.print(a + " ");
	}
     }
public static void main(String args[])
	{
		Solution obj=new Solution();
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		obj.Fibinocci_in_Reverse_order(n,0,1);
		
	}
}
