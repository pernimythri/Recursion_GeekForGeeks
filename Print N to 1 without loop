Time Complexity : O(N)

import java.util.*;
class Solution
{
    static void PrintNumbers(int n)
    {
        if(n>0)
        {
            System.out.println(n+" ");
            PrintNumbers(n-1);
    }
}
public static void main(String[] args)
{
    Solution obj = new Solution();
    Scanner sc = new Scanner(System.in);
    System.out.print("Enter value of n : ");
    int n=sc.nextInt();
    obj.PrintNumbers(n);
}
}

